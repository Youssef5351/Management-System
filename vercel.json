{
    "version": 2,
    "builds": [
      { "src": "client/package.json", "use": "@vercel/static-build" },
      { "src": "server/src/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/src/api/$1.js" },
      { "src": "/(.*)", "dest": "client/$1" }
    ]
  }